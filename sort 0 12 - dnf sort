void sort012(int a[], int n)
    {
    int start=0,end=n-1; 
    int i=0;
    while(start<end and i<=end){
      if(a[i]==0){
          swap(a[start],a[i]);
          start++;
          i++;
      }
      else if(a[i]==1)
      {
        i++;
      }
      else if(a[i]==2)
      {
          swap(a[i],a[end]);
          end--;
      }
    }
